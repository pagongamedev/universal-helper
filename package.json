{
  "name": "universal-helper",
  "description": "helper for fullstack developer",
  "version": "0.0.28",
  "type": "module",
  "files": [
    "dist",
    "css"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/universal-helper.js",
  "main": "./dist/universal-helper.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/universal-helper.js",
      "require": "./dist/universal-helper.umd.cjs"
    },
    "./css/": {
      "import": "./css/",
      "require": "./css/"
    }
  },
  "engines": {
    "npm": "please-use-pnpm",
    "node": ">=19.0.0 <20.0.0",
    "pnpm": ">=8.0.0 <9.0.0"
  },
  "scripts": {
    "start": "vite build --watch",
    "tsc": "tsc --watch",
    "build": "tsc && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pagongamedev/universal-helper.git"
  },
  "keywords": [
    "helper",
    "javascirpt",
    "node",
    "fullstack",
    "frontend",
    "backend"
  ],
  "author": "pagongamedev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pagongamedev/universal-helper/issues"
  },
  "homepage": "https://github.com/pagongamedev/universal-helper#readme",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "dayjs": "^1.11.7",
    "firebase": "^9.19.1",
    "i18next": "^22.4.14",
    "react": "^18.2.0",
    "react-i18next": "^12.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "zustand": "^4.3.7"
  },
  "peerDependencies": {
    "i18next": "^22.X",
    "react": "^18.X",
    "react-i18next": "^12.X",
    "zustand": "^4.X",
    "firebase": "^9.X",
    "dayjs": "^1.X"
  }
}
